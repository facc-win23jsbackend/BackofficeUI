@using BackOfficeUI.Client.Models

@code {
    [Parameter] public AuthorModel Author { get; set; }
}

<div class="form-group">
    <label>Name</label>
    <InputText @bind-Value="Author.Name"></InputText>
</div>
<div class="form-group">
    <label>Image Url</label>
    <InputText @bind-Value="Author.AuthorImageUri"></InputText>
</div>